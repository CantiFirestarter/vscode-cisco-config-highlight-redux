{
  "name": "cisco-config-highlight",
  "displayName": "Cisco Config Highlight",
  "description": "Cisco device configuration Syntax Highlighting for Visual Studio Code.",
  "version": "0.6.1",
  "publisher": "Y-Ysss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Y-Ysss/vscode-cisco-config-highlight.git"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "images/icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "cisco",
        "aliases": [
          "Cisco Config",
          "cisco"
        ],
        "extensions": [
          ".cisco",
          ".config"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cisco",
        "scopeName": "source.cisco",
        "path": "./syntaxes/cisco.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "cisco-config-highlight.applyColors",
        "title": "Cisco Config: Apply Token Colors",
        "category": "Cisco Config Highlight"
      }
    ],
    "configuration": [
      {
        "title": "Cisco Config Highlight",
        "properties": {
          "cisco-config-highlight.applyAllTokenRules": {
            "type": "boolean",
            "default": false,
            "markdownDescription": "Apply all token scopes from syntaxes/textMateRules.json on activation and configuration changes. User color settings override these defaults by scope."
          },
          "cisco-config-highlight.outline.showSymbolsInOutlinePanel": {
            "type": "boolean",
            "default": false,
            "description": "%configuration.properties.showSymbolsInOutlinePanel.description%"
          },
          "cisco-config-highlight.outline.symbolsList": {
            "type": "object",
            "properties": {
              "command": {
                "type": "boolean",
                "default": true,
                "description": "%configuration.properties.symbols.command.description%"
              },
              "ip_vrf": {
                "type": "boolean",
                "default": true,
                "description": "%configuration.properties.symbols.ipVrf.description%"
              },
              "router_bgp": {
                "type": "boolean",
                "default": true,
                "description": "%configuration.properties.symbols.routerBgp.description%"
              },
              "address_family": {
                "type": "boolean",
                "default": true,
                "description": "%configuration.properties.symbols.addressFamily.description%"
              },
              "class_map": {
                "type": "boolean",
                "default": true,
                "description": "%configuration.properties.symbols.classMap.description%"
              },
              "policy_map": {
                "type": "boolean",
                "default": true,
                "description": "%configuration.properties.symbols.policyMap.description%"
              },
              "interface": {
                "type": "boolean",
                "default": true,
                "description": "%configuration.properties.symbols.interface.description%"
              },
              "sub_interface": {
                "type": "boolean",
                "default": true,
                "description": "%configuration.properties.symbols.subInterface.description%"
              }
            },
            "additionalProperties": false,
            "default": {
              "command": true,
              "ip_vrf": true,
              "router_bgp": true,
              "address_family": true,
              "class_map": true,
              "policy_map": true,
              "interface": true,
              "sub_interface": true
            },
            "description": "%configuration.properties.showEnableDisableSymbols.description%"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint -c eslint.config.mjs \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint -c eslint.config.mjs \"src/**/*.{js,ts}\" --fix",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "apply-token-scopes": "node scripts/applyScopeRules.js",
    "apply-token-scopes:ts": "ts-node scripts/applyScopeRules.ts",
    "apply-token-scopes:dry-run": "node scripts/applyScopeRules.js --dry-run"
  },
  "dependencies": {
    "semver": "^7.5.2"
  },
  "devDependencies": {
    "@types/semver": "^7.5.2",
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.63.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "ts-node": "^10.9.2",
    "prettier": "^3.2.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0"
  },
  "__metadata": {
    "installedTimestamp": 1765500322524,
    "targetPlatform": "undefined",
    "size": 445484
  }
}
