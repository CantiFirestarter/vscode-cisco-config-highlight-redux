{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Cisco Config",
  "scopeName": "source.cisco",
  "patterns": [
    { "include": "#unused_line" },
    { "include": "#comment" },
    { "include": "#interface_type" },
    { "include": "#vrf" },
    { "include": "#string" },
    { "include": "#group" },
    { "include": "#acl" },
    { "include": "#crypto" },
    { "include": "#address" },
    { "include": "#arp_insp_val" },
    { "include": "#command_hostname" },
    { "include": "#numeric" },
    { "include": "#separator" },
    {
      "comment": "bgp must be placed after address. {neighbor [peer-group-name or ip-address]}",
      "include": "#bgp"
    },
    { "include": "#keyword" }
  ],
  "repository": {
    "unused_line": {
      "patterns": [{ "include": "#unused" }, { "include": "#default" }],
      "repository": {
        "unused": {
          "name": "meta.function-call.command-disable.unused.cisco",
          "match": "(?i)^\\s*?no\\s.*"
        },
        "default": {
          "name": "meta.function-call.command-disable.default.cisco",
          "match": "(?i)^\\s*?default\\s.*"
        }
      }
    },
    "comment": {
      "patterns": [
        {
          "name": "comment.line.config.cisco",
          "match": "^\\s*[!#].*"
        },
        {
          "name": "comment.line.config.cisco",
          "begin": "(?<=\\s)[!#]",
          "end": "$"
        }
      ]
    },
    "interface_type": {
      "patterns": [
        { "include": "#ethernet" },
        { "include": "#wireless" },
        { "include": "#loopback" },
        { "include": "#port-channel" },
        { "include": "#tunnel" },
        { "include": "#vlan" },
        { "include": "#null" },
        { "include": "#serial" },
        { "include": "#cellular" },
        { "include": "#virtual-template" },
        { "include": "#dialer" },
        { "include": "#async" },
        { "include": "#bri" },
        { "include": "#bvi" },
        { "include": "#app" },
        { "include": "#management" }
      ],
      "repository": {
        "ethernet": {
          "name": "entity.name.class.interface.ethernet.cisco",
          "match": "(?i)\\b((fast|(five|ten|twentyfive|f(orty|ifty)|(two)?hundred)?g(ig)?(abit)?)?e(th)?(ernet)?|fa|gi|te|fo|fi|g)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)(-\\d+(\\.\\d+)?)?\\b"
        },
        "wireless": {
          "name": "entity.name.class.interface.wireless.cisco",
          "match": "(?i)\\b(dot11radio)(([0-1]([\\./]\\d)?(\\.\\d+)?)\\b(\\.\\d+)?)?\\b"
        },
        "loopback": {
          "name": "entity.name.class.interface.loopback.cisco",
          "match": "(?i)\\b(loopback|lo)(\\d+)?\\b"
        },
        "port-channel": {
          "name": "entity.name.class.interface.portchannel.cisco",
          "match": "(?i)\\b(port-?channel|po)(12[0-8]|1[0-1][0-9]|[1-9][0-9]|[1-9])?\\b"
        },
        "tunnel": {
          "name": "entity.name.class.interface.tunnel.cisco",
          "match": "(?i)\\b(tunnel|tu)(\\d+)?\\b"
        },
        "vlan": {
          "name": "entity.name.class.interface.vlan.cisco",
          "match": "(?i)\\b(vlan)(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])?\\b"
        },
        "null": {
          "name": "entity.name.class.interface.null.cisco",
          "match": "(?i)\\b(null)(\\d+)?\\b"
        },
        "serial": {
          "name": "entity.name.class.interface.serial.cisco",
          "match": "(?i)\\b(serial|s)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b"
        },
        "cellular": {
          "name": "entity.name.class.interface.cellular.cisco",
          "match": "(?i)\\b(cellular)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b"
        },
        "virtual-template": {
          "name": "entity.name.class.interface.virtual-template.cisco",
          "match": "(?i)\\b(virtual-template)(\\d+)?\\b"
        },
        "dialer": {
          "name": "entity.name.class.interface.dialer.cisco",
          "match": "(?i)\\b(dialer)(\\d+)?\\b"
        },
        "async": {
          "name": "entity.name.class.interface.async.cisco",
          "match": "(?i)\\b(async)(\\d+)?\\b"
        },
        "bri": {
          "name": "entity.name.class.interface.bri.cisco",
          "match": "(?i)\\b(bri)(\\d+)?\\b"
        },
        "bvi": {
          "name": "entity.name.class.interface.bvi.cisco",
          "match": "(?i)\\b(bvi)(\\d+)?\\b"
        },
        "app": {
          "name": "entity.name.class.interface.app.cisco",
          "match": "(?i)\\b((app?g(ig)?(abit)?)?e(th)?(ernet)?|ap)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b"
        },
        "management": {
          "name": "entity.name.class.interface.management.cisco",
          "match": "(?i)\\b(management|mgmt)((\\d+[\\./])*)?(\\d+)?\\b"
        }
      }
    },
    "vrf": {
      "patterns": [
        {
          "comment": "((?<=vrf\\s)(?!.*forwarding)|(?<=vrf\\sforwarding\\s))\\S+",
          "match": "(?i)(vrf)\\s((forwarding|definition)\\s)?(\\S+)",
          "captures": {
            "1": { "name": "entity.name.class.vrf.declaration.cisco" },
            "2": { "name": "entity.other.vrf.forwarding.cisco" },
            "3": { "name": "entity.other.vrf.definition.cisco" },
            "4": { "name": "entity.name.tag.vrf.vrf-name.cisco" }
          }
        }
      ]
    },
    "string": {
      "patterns": [
        { "include": "#hostname" },
        { "include": "#description" },
        { "include": "#remark" },
        { "include": "#domain-name" },
        { "include": "#username" },
        { "include": "#name" },
        { "include": "#logging-system-message" },
        { "include": "#password" },
        { "include": "#secret" },
        { "include": "#banner" }
      ],
      "repository": {
        "description": {
          "match": "(?i)(?<=(description))\\s(.*)$",
          "name": "string.other.description.cisco"
        },
        "remark": {
          "match": "(?i)(?<=remark)\\s(.*)$",
          "name": "string.other.remark.cisco"
        },
        "hostname": {
          "match": "(?i)(?<=hostname\\s)[0-9a-zA-Z\\-]+\\b",
          "name": "entity.name.tag.config-string.hostname.cisco"
        },
        "domain-name": {
          "match": "(?i)(?<=ip\\sdomain\\sname\\s)\\S+\\b",
          "name": "entity.name.tag.config-string.domain-name.cisco"
        },
        "username": {
          "match": "(?i)(?<=username\\s)(\\S+)\\b",
          "name": "entity.name.tag.config-string.username.cisco"
        },
        "name": {
          "match": "(?i)(?<=name\\s)(\\S+)\\b",
          "name": "entity.name.tag.config-string.name.cisco"
        },
        "logging-system-message": {
          "match": "(?<=%)\\S+-\\d-\\S+(?=:)",
          "name": "entity.name.tag.config-string.logging-system-message.cisco"
        },
        "password": {
          "match": "(?i)(?<=password\\s)(?:\\d+\\s)?(\\S+)$",
          "name": "string.other.password.cisco"
        },
        "secret": {
          "match": "(?i)(?<=secret\\s)(?:\\d+\\s)?(\\S+)$",
          "name": "string.other.secret.cisco"
        },
        "banner": {
          "begin": "(?i)(?<=banner\\s(exec|login|motd)\\s)(\\^|@)",
          "end": "(\\^|@)",
          "name": "comment.block.banner.cisco"
        }
      }
    },
    "group": {
      "patterns": [{ "include": "#group-modules" }],
      "repository": {
        "group-modules": {
          "patterns": [
            { "include": "#qos-module" },
            { "include": "#routing-module" },
            { "include": "#bgp-module" },
            { "include": "#security-module" },
            { "include": "#traffic-module" },
            { "include": "#resource-module" }
          ]
        },
        "qos-module": {
          "patterns": [
            {
              "match": "(?i)(?<=class\\s(.*\\s)?)(\\S+)$",
              "name": "entity.name.tag.group.qos.class.name.cisco"
            },
            {
              "match": "(?i)(?<=service-policy\\s(.*\\s)?)(\\S+)$",
              "name": "entity.name.tag.group.qos.service-policy.name.cisco"
            },
            {
              "match": "(?i)(?<=policy-map\\s(.*\\s)?)(\\S+)$",
              "name": "entity.name.tag.group.qos.policy-map.name.cisco"
            },
            {
              "match": "(?i)(?<=class-map\\s(.*\\s)?)(\\S+)$",
              "name": "entity.name.tag.group.qos.class-map.name.cisco"
            }
          ]
        },
        "routing-module": {
          "patterns": [
            {
              "match": "(?i)(?<=route-map\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.routing.route-map.name.cisco"
            },
            {
              "match": "(?i)(?<=prefix-list\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.routing.prefix-list.name.cisco"
            }
          ]
        },
        "bgp-module": {
          "patterns": [
            {
              "match": "(?i)(?<=community\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.bgp.community.name.cisco"
            }
          ]
        },
        "security-module": {
          "patterns": [
            {
              "match": "(?i)(?<=object-group\\s)((service|network)\\s)?(\\S+)\\b",
              "captures": {
                "2": {
                  "name": "keyword.other.group.security.object-group.type.cisco"
                },
                "3": {
                  "name": "entity.name.tag.group.security.object-group.name.cisco"
                }
              }
            }
          ]
        },
        "traffic-module": {
          "patterns": [
            {
              "match": "(?i)(?<=traffic-filter\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.traffic.traffic-filter.name.cisco"
            }
          ]
        },
        "resource-module": {
          "patterns": [
            {
              "match": "(?i)(?<=\\bpool\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.resource.pool.name.cisco"
            },
            {
              "match": "(?i)(?<=policy-list\\s)(\\S+)\\b",
              "name": "entity.name.tag.group.resource.policy-list.name.cisco"
            }
          ]
        }
      }
    },
    "acl": {
      "patterns": [
        { "include": "#access-class" },
        { "include": "#access-list" },
        { "include": "#access-group" },
        { "include": "#acl-protocols" },
        { "include": "#acl-tcp-flags" },
        { "include": "#acl-icmp-types" },
        { "include": "#acl-options" },
        { "include": "#acl-port-operators" }
      ],
      "repository": {
        "access-list": {
          "match": "(?i)(ipv6\\s+)?(ip\\s+)?(access-list)\\s+(?:(extended|standard)\\s+)?(\\S+)\\b",
          "captures": {
            "1": { "name": "keyword.other.config-keyword.routing-protocols.cisco" },
            "2": { "name": "keyword.other.config-keyword.config-commands.cisco" },
            "3": { "name": "keyword.other.config-keyword.config-commands.cisco" },
            "4": { "name": "keyword.other.acl.access-list.type.cisco" },
            "5": { "name": "entity.name.tag.acl.access-list.name.cisco" }
          }
        },
        "access-class": {
          "match": "(?i)(ipv6\\s+)?(access-class)\\s+(\\S+)(?:\\s+(in|out))?\\b",
          "captures": {
            "1": { "name": "keyword.other.config-keyword.routing-protocols.cisco" },
            "2": { "name": "keyword.other.config-keyword.config-commands.cisco" },
            "3": { "name": "entity.name.tag.acl.access-class.name.cisco" },
            "4": { "name": "keyword.other.acl.access-class.type.cisco" }
          }
        },
        "access-group": {
          "match": "(?i)(ipv6\\s+)?((?:ip\\s+)?access-group)\\s+(\\S+)(?:\\s+(in|out))?\\b",
          "captures": {
            "1": { "name": "keyword.other.config-keyword.routing-protocols.cisco" },
            "2": { "name": "keyword.other.config-keyword.config-commands.cisco" },
            "3": { "name": "entity.name.tag.acl.access-group.name.cisco" },
            "4": { "name": "keyword.other.acl.access-group.type.cisco" }
          }
        },
        "acl-protocols": {
          "name": "keyword.other.acl.protocol.cisco",
          "match": "(?i)\\b(gre|esp|ahp|pim|ipinip|nos|icmpv6|udp|igmp|pcp|sctp)\\b"
        },
        "acl-tcp-flags": {
          "name": "keyword.other.acl.tcp-flag.cisco",
          "match": "(?i)\\b(ack|fin|psh|rst|syn|urg)\\b"
        },
        "acl-icmp-types": {
          "name": "keyword.other.acl.icmp-type.cisco",
          "match": "(?i)\\b(mobile-registration-request|mobile-registration-reply|datagram-conversion-error|domain-name-request|domain-name-reply|ipv6-where-are-you|mobile-host-redirect|router-advertisement|router-solicitation|timestamp-request|timestamp-reply|information-request|information-reply|parameter-problem|mobile-redirect|ipv6-i-am-here|conversion-error|alternate-address|time-exceeded|source-quench|mask-request|mask-reply|echo-reply|unreachable|traceroute|redirect|photuris|skip|echo)(?=\\s|\\b|$)"
        },
        "acl-options": {
          "name": "keyword.other.acl.option.cisco",
          "match": "(?i)\\b(time-range|precedence|tos|ttl|packet-length|reflect|evaluate|dynamic|timeout|log|option|sequence)(?=\\s|\\b|$)"
        },
        "acl-port-operators": {
          "name": "keyword.other.acl.port-operator.cisco",
          "match": "(?i)\\b(neq|lt|range)\\b"
        }
      }
    },
    "crypto": {
      "patterns": [
        { "include": "#crypto-map" },
        { "include": "#transform-set" },
        { "include": "#ipsec-profile" },
        { "include": "#isakmp-profile" },
        { "include": "#keyring" }
      ],
      "repository": {
        "crypto-map": {
          "match": "(?i)(?<=\\bcrypto\\smap\\s)(\\S+)\\b",
          "name": "entity.name.tag.crypto.crypto-map.name.cisco"
        },
        "transform-set": {
          "match": "(?i)(?<=\\btransform-set\\s)(\\S+\\s?)+\\b",
          "name": "entity.name.tag.crypto.transform-set.name.cisco"
        },
        "ipsec-profile": {
          "match": "(?i)(?<=\\bipsec\\sprofile\\s)(\\S+)\\b",
          "name": "entity.name.tag.crypto.ipsec-profile.name.cisco"
        },
        "isakmp-profile": {
          "match": "(?i)(?<=\\bisakmp\\sprofile\\s)(\\S+)\\b",
          "name": "entity.name.tag.crypto.isakmp-profile.name.cisco"
        },
        "keyring": {
          "match": "(?i)(?<=\\bkeyring\\s)(\\S+)\\b",
          "name": "entity.name.tag.crypto.keyring.name.cisco"
        }
      }
    },
    "address": {
      "patterns": [
        { "include": "#mac" },
        { "include": "#cidr" },
        { "include": "#ipv4-AD" },
        { "include": "#ipv6-AD" },
        { "include": "#ipv4-full" },
        { "include": "#ipv6-full" },
        { "include": "#ipv6-condensed" }
      ],
      "repository": {
        "mac": {
          "patterns": [
            {
              "name": "keyword.other.address.mac.cisco",
              "comment": "Standard MAC format with enforced consistent separator: AA:BB:CC:DD:EE:FF / AA-BB-CC-DD-EE-FF / AA.BB.CC.DD.EE.FF",
              "match": "\\b([0-9a-fA-F]{2})([:.-])([0-9a-fA-F]{2})(\\2)([0-9a-fA-F]{2})(\\2)([0-9a-fA-F]{2})(\\2)([0-9a-fA-F]{2})(\\2)([0-9a-fA-F]{2})\\b",
              "captures": {
                "2": { "name": "punctuation.separator.cisco" },
                "4": { "name": "punctuation.separator.cisco" },
                "6": { "name": "punctuation.separator.cisco" },
                "8": { "name": "punctuation.separator.cisco" },
                "10": { "name": "punctuation.separator.cisco" }
              }
            },
            {
              "name": "keyword.other.address.mac.cisco",
              "comment": "Cisco dotted MAC format: AABB.CCDD.EEFF",
              "match": "\\b([0-9a-fA-F]{4})([.])([0-9a-fA-F]{4})([.])([0-9a-fA-F]{4})\\b",
              "captures": {
                "2": { "name": "punctuation.separator.cisco" },
                "4": { "name": "punctuation.separator.cisco" }
              }
            }
          ]
        },
        "cidr": {
          "name": "keyword.other.address.cidr.cisco",
          "match": "(?<=(?:(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)|[0-9a-fA-F]{1,4}|::)\\/)(?:12[0-8]|1[01]\\d|[1-9]?\\d)\\b"
        },
        "ipv4-AD": {
          "name": "constant.numeric.ipv4-AD.cisco",
          "match": "(?<=\\b(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\.(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\.(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\.(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\s)(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]\\d?)$"
        },
        "ipv6-AD": {
          "name": "constant.numeric.ipv6-AD.cisco",
          "match": "(?<=(?:(?:[0-9a-fA-F]{1,4}:)*[0-9a-fA-F]{0,4}::(?:[0-9a-fA-F]{1,4}:)*[0-9a-fA-F]{0,4}|(?:[0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}|::)\\s)(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]\\d?)$"
        },
        "ipv4-full": {
          "name": "keyword.other.address.ipv4.full.cisco",
          "match": "\\b(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)(\\.)(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)(\\.)(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)(\\.)(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\b",
          "captures": {
            "2": { "name": "punctuation.separator.cisco" },
            "4": { "name": "punctuation.separator.cisco" },
            "6": { "name": "punctuation.separator.cisco" }
          }
        },
        "ipv6-full": {
          "name": "keyword.other.address.ipv6.full.cisco",
          "match": "\\b([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})\\b",
          "captures": {
            "2": { "name": "punctuation.separator.cisco" },
            "4": { "name": "punctuation.separator.cisco" },
            "6": { "name": "punctuation.separator.cisco" },
            "8": { "name": "punctuation.separator.cisco" },
            "10": { "name": "punctuation.separator.cisco" },
            "12": { "name": "punctuation.separator.cisco" },
            "14": { "name": "punctuation.separator.cisco" }
          }
        },
        "ipv6-condensed": {
          "comment": "IPv6 condensed format with :: compression. Matches addresses with hex groups and :: notation.",
          "match": "(?:^|\\s|(?<=[^0-9a-fA-F:]))(?:(?:[0-9a-fA-F]{1,4}:)*[0-9a-fA-F]{0,4}::(?:[0-9a-fA-F]{1,4}:)*[0-9a-fA-F]{0,4}|(?:[0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}|::)(?=\\s|[^0-9a-fA-F:]|$)",
          "captures": {
            "0": {
              "patterns": [
                {
                  "match": ":",
                  "name": "punctuation.separator.cisco"
                },
                {
                  "match": "[0-9a-fA-F]+",
                  "name": "keyword.other.address.ipv6.condensed.cisco"
                }
              ]
            }
          }
        }
      }
    },
    "arp_insp_val": {
      "begin": "(?i)(?<=ip arp inspection validate\\s)",
      "end": "$",
      "name": "meta.function-call.arp-insp-val.cisco",
      "patterns": [
        {
          "match": "(?i)\\bsrc-?mac\\b",
          "name": "constant.other.arp-insp-val.src-mac.cisco"
        },
        {
          "match": "(?i)\\bdst-?mac\\b",
          "name": "constant.other.arp-insp-val.dst-mac.cisco"
        },
        {
          "match": "(?i)\\bip\\b",
          "name": "constant.other.arp-insp-val.ip.cisco"
        }
      ]
    },
    "command_hostname": {
      "patterns": [
        {
          "name": "meta.function-call.command_hostname.user-mode.cisco",
          "match": "^(?!\\s)[0-9a-zA-Z\\-]+(\\(.*\\))?#"
        },
        {
          "name": "meta.function-call.command_hostname.privileged-mode.cisco",
          "match": "^(?!\\s)[0-9a-zA-Z\\-]+(\\(.*\\))?>"
        }
      ]
    },
    "numeric": {
      "patterns": [{ "include": "#integer" }, { "include": "#hex" }],
      "repository": {
        "hex": {
          "name": "constant.numeric.hex.cisco",
          "match": "\\b0x[0-9A-Fa-f]+\\b|(?<![:\\-.a-zA-Z])[0-9A-Fa-f]{2}(?![:\\-.0-9A-Fa-f]|[a-zA-Z])"
        },
        "integer": {
          "name": "constant.numeric.integer.cisco",
          "match": "(?<!\\.)\\b\\d+\\b(?![\\.\\:@])"
        }
      }
    },
    "separator": {
      "patterns": [
        {
          "name": "punctuation.separator.cisco",
          "match": "[|\\/:.,\\-<>]"
        }
      ]
    },
    "bgp": {
