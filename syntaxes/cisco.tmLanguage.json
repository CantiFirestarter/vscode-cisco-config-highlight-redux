{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Cisco Config",
  "scopeName": "source.cisco",
  "patterns": [
    { "include": "#unused_line" },
    { "include": "#comment" },
    { "include": "#interface_type" },
    { "include": "#interface" },
    { "include": "#vrf" },
    { "include": "#string" },
    { "include": "#group" },
    { "include": "#acl" },
    { "include": "#crypto" },
    { "include": "#address" },
    { "include": "#command_hostname" },
    { "include": "#numeric" },
    { "include": "#separator" },
    {
      "comment": "bgp must be placed after address. {neighbor [peer-group-name or ip-address]}",
      "include": "#bgp"
    },
    { "include": "#keyword" },
    { "include": "#param1" },
    { "include": "#param2" }
  ],
  "repository": {
    "separator": {
      "patterns": [
        {
          "name": "punctuation.separator.cisco",
          "match": "[|\\/:.,\\-<>]"
        }
      ]
    },
    "keyword": {
      "patterns": [
        {
          "name": "keyword.other.config-keyword.vlan.cisco",
          "match": "(?i)(^|\\s)(protocol-vlan|mac-vlan|dot1q-tunnel|dot1[Qq])(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.switchport-mode.access.cisco",
          "match": "(?i)(^|\\s)(access)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.switchport-mode.trunk.cisco",
          "match": "(?i)(^|\\s)(trunk)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.switchport-mode.dynamic.cisco",
          "match": "(?i)(^|\\s)(dynamic\\s(auto|desirable))(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.add-remove.add.cisco",
          "match": "(?i)(^|\\s)(add)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.add-remove.remove.cisco",
          "match": "(?i)(^|\\s)(remove)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.add-remove.except.cisco",
          "match": "(?i)(^|\\s)(except)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.allowed-native.cisco",
          "match": "(?i)(^|\\s)(allowed|native)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.any-all.any.cisco",
          "match": "(?i)(^|\\s)(any)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.any-all.all.cisco",
          "match": "(?i)(^|\\s)(all)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.permit-deny.permit.cisco",
          "match": "(?i)(^|\\s)(permit)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.permit-deny.deny.cisco",
          "match": "(?i)(^|\\s)(deny)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.status.up.cisco",
          "match": "(?i)(^|\\s)(up)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.status.down.cisco",
          "match": "(?i)(^|\\s)(down)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.status.deleted.cisco",
          "match": "(?i)(^|\\s)(deleted)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.status.administratively-down.cisco",
          "match": "(?i)(^|\\s)(administratively(-down)?)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.match.any.cisco",
          "match": "(?i)(^|\\s)(match-(any))(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.match.all.cisco",
          "match": "(?i)(^|\\s)(match-(all))(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.in-out.in.cisco",
          "match": "(?i)(^|\\s)(in)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.in-out.out.cisco",
          "match": "(?i)(^|\\s)(out)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.input-output.input.cisco",
          "match": "(?i)(^|\\s)(input)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.input-output.output.cisco",
          "match": "(?i)(^|\\s)(output)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.inside-outside.inside.cisco",
          "match": "(?i)(^|\\s)(inside)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.inside-outside.outside.cisco",
          "match": "(?i)(^|\\s)(outside)(?=\\s|$)"
        },
        {
          "name": "keyword.other.config-keyword.shutdown.cisco",
          "match": "(?i)(^|\\s)(shutdown)(?=\\b|$)"
        },
        {
          "name": "keyword.other.config-keyword.remark.cisco",
          "match": "(?i)(^|\\s)(remark)(?=\\b|$)"
        },
        {
          "name": "keyword.other.config-keyword.group1.cisco",
          "match": "(?i)(^|\\s)(aaa_server|access-log|acct-port|accounting|action|address|admission|advertise-v2|age|aes-ccm|allocation|ap|attach-policy|ascending|auth|auth-port|auth-proxy|authenticate-fail|authentication-server|authentication|authorization|arp|auto|auto-sync|aux|basic-summary-address|begin|block-unknown-source|bootpc|bootps|bootup|bpdufilter|bpduguard|broadcast|brief|bus|cache|capability-change|cck|ciphers|change|chain|chassis-id|cdp|certificate|cef|client|client-id|classless|coldstart|compress-config|config|config-copy|connection|contact|conflict|console|cor|counters|country|crypto|cs4|dead-criteria|datetime|debug|deauthenticate|destination|device|dhcp|dial-peer|disassociate|dot1Q|dot11-qos|dot11d|eap|edge|entries|enrollment|entity|exec-time|exec|extended|extendable|excluded-address|exclude|eq|file|finrst-timeout|flow-aggregation|flow-export|flow|flowcontrol|forward-protocol|fallback|flash|general-prefix|guest-mode|Guest|group|h-node|help-path|helper-address|host|icmp|include|included|indoor|ingress|infrastructure|inside|ipsec|irb|iso|key-management|key|level|linkdown|linkup|list|local|localtime|logging|login|log|location|mac-address|main-cpu|max|membership-termination|mgmt|msec|missing|min-length|minimal|mode|modulus|neighbor|next-hop|native|new-model|newinfo|nde-address|netbios-dgm|netbios-ns|netflow|network|nonegotiate|nat-transparency|nat|ofdm|optional|open|page|pad|password-encryption|passwords|password|pki|pool|port-security|primary|prefer|priority|preempt|prefix|proxy|proxy-arp|pvst|qos|ra|radius|range|rapid-pvst|ratelimit|recurring|redirects|recovery|redundancy|resource|router-id|routing|route|route-cache|running-config|rp|root|rogue-ap|run|secure-server|send|self-signed|selfsigned|service-type|session-id|section|secret|snmptrap|sso|start-stop|static|station-role|spanning-disabled|subscriber-loop-control|subnet-zero|subject-name|summary-address|summary|switch-over|syslog|tacacs+|tcp-timeout|time|timezone|traps|trunk|traffic-filter|translation|translations|transport|tries|threshold|tty|tkip|tracking|type|udp-encaps|udp-port|udp|unicast-flooding|unicast-routing|unreachables|update-calendar|update|usage|uptime|v3|vsa|vty|view|version|voice|wds|wep|wep128|wlan-wep|window-size|warmstart|wnm|write|wpa|Z)(?=\\b|$)"
        },
        {
          "name": "keyword.other.config-keyword.group2.cisco",
          "match": "(?i)(^|\\s)(access-class|access-list|algorithm|algorithm-type|autostate|banner|boot|bpdu|broadcast-key|bridge-group|bridge|channel-group|channel-protocol|channel|class|class-map|client-identifier|configure|copy|default-router|description|device-tracking|diagnostic|dir|dns-server|domain-lookup|domain-name|domain|dot11|dot1x|duplex|encryption|encapsulation|errdisable|group2aaa|hostname|input|int|install|interface|IP-Address|ip|lease|line|lldp|license|mbssid|method|mls|mpls|name|nd|netbios-name-server|netbios-node-type|neighbors|ntp|ok\\?|police|policy-map|power|process|reauth-period|redistribute|request|route-map|router|rsa|rts|security|service-policy|service|sntp|software|spanning-tree|speed|startup-config|storm-control|stackwise-virtual|sync|switch|switchport|tacacs-server|udld|vtp|wlccp|wr|world-mode|username|vrf)(?=\\b|$)"
        },
        {
          "name": "keyword.other.config-keyword.group3.cisco",
          "match": "(?i)(^|\\s)(timeout|inactive|exec-timeout|session-timeout|outside|out|exceed-action|drop|expired|failure|stop-record|boot-end-marker|error|reset|end|unassigned|unset|shutdown|system-shutdown|exit|none|limit|down|removal|removed|on-failure|cause|gbic-invalid|off|clear|standby|violation|misconfig|exit-address-family|exception|crashinfo|zeroize|erase|ignore|max-fail|theft|delete)(?=\\b|$)"
        },
        {
          "name": "keyword.other.config-keyword.group4.cisco",
          "match": "(?i)(^|\\s)(addon|activate|active|aging|allow|allowed|authenticate|authentication-key|auto-copy|auto-upgrade|binding|boot-start-marker|both|clean|commit|conform-action|complete|detail|desirable|enable|enabled|established|full|generate|gt|in|insertion|maximun|optimize|on|on-success|permit|receive|relay|reload|ro|rw|set|show|send-community|success|trust|transmit|trusted-key|update-source|verify|yes)(?=\\b|$)"
        },
        {
          "name": "keyword.other.config-keyword.group5.cisco",
          "match": "(?i)(^|\\s)(address-family|allow-hint|automatic|buffered|channel-misconfig|debugging|default-gateway|etherchannel|general-keys|if-authenticated|inspection|interval|IPDT_POLICY|label|link-local|lookup|managed-config-flag|md5|motd|mtu|negotiation|no-autoconfig|option|other-config-flag|packet|portfast|provision|renumber|route-target|scrypt|smart|snooping|standard|sticky|stopbits|terminal|transparent)(?=\\b|$)"
        },
        {
          "name": "keyword.other.config-keyword.group6.cisco",
          "match": "(?i)(^|\\s)(all|auto-summary|bgp|default|dscp|echo|eigrp|fragments|ftp|ftp-data|gopher|http|ident|imap|isakmp|ipv4|ipv6|lacp|log-input|nntp|ospf|pagp|ping|pop3|rip|scp|snmp|snmp-server|smtp|sonet|ssh|telnet|tcp|tftp|www)(?=\\b|$)"
        }
      ]
    },
    "interface_type": {
      "patterns": [
        { "include": "#ethernet" },
        { "include": "#wireless" },
        { "include": "#loopback" },
        { "include": "#port-channel" },
        { "include": "#tunnel" },
        { "include": "#vlan" },
        { "include": "#null" },
        { "include": "#serial" },
        { "include": "#cellular" },
        { "include": "#virtual-template" },
        { "include": "#dialer" },
        { "include": "#async" },
        { "include": "#bri" },
        { "include": "#bvi" },
        { "include": "#app" },
        { "include": "#management" }
      ],
      "repository": {
        "ethernet": {
          "name": "entity.name.class.interface.ethernet.cisco",
          "match": "(?i)\\b((fast|(five|ten|twentyfive|f(orty|ifty)|(two)?hundred)?g(ig)?(abit)?)?e(th)?(ernet)?|fa|gi|te|fo|fi|g)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)(-\\d+(\\.\\d+)?)?\\b"
        },
        "wireless": {
          "name": "entity.name.class.interface.wireless.cisco",
          "match": "(?i)\\b(dot11radio)(([0-1]([\\./]\\d)?(\\.\\d+)?)\\b(\\.\\d+)?)?\\b"
        },
        "loopback": {
          "name": "entity.name.class.interface.loopback.cisco",
          "match": "(?i)\\b(loopback|lo)(\\d+)?\\b"
        },
        "port-channel": {
          "name": "entity.name.class.interface.portchannel.cisco",
          "match": "(?i)\\b(port-?channel|po)(12[0-8]|1[0-1][0-9]|[1-9][0-9]|[1-9])?\\b"
        },
        "tunnel": {
          "name": "entity.name.class.interface.tunnel.cisco",
          "match": "(?i)\\b(tunnel|tu)(\\d+)?\\b"
        },
        "vlan": {
          "name": "entity.name.class.interface.vlan.cisco",
          "match": "(?i)\\b(vlan)(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])?\\b"
        },
        "null": {
          "name": "entity.name.class.interface.null.cisco",
          "match": "(?i)\\b(null)(\\d+)?\\b"
        },
        "serial": {
          "name": "entity.name.class.interface.serial.cisco",
          "match": "(?i)\\b(serial|s)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b"
        },
        "cellular": {
          "name": "entity.name.class.interface.cellular.cisco",
          "match": "(?i)\\b(cellular)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b"
        },
        "virtual-template": {
          "name": "entity.name.class.interface.virtual-template.cisco",
          "match": "(?i)\\b(virtual-template)(\\d+)?\\b"
        },
        "dialer": {
          "name": "entity.name.class.interface.dialer.cisco",
          "match": "(?i)\\b(dialer)(\\d+)?\\b"
        },
        "async": {
          "name": "entity.name.class.interface.async.cisco",
          "match": "(?i)\\b(async)(\\d+)?\\b"
        },
        "bri": {
          "name": "entity.name.class.interface.bri.cisco",
          "match": "(?i)\\b(bri)(\\d+)?\\b"
        },
        "bvi": {
          "name": "entity.name.class.interface.bvi.cisco",
          "match": "(?i)\\b(bvi)(\\d+)?\\b"
        },
        "app": {
          "name": "entity.name.class.interface.app.cisco",
          "match": "(?i)\\b((app?g(ig)?(abit)?)?e(th)?(ernet)?|ap)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b"
        },
        "management": {
          "name": "entity.name.class.interface.management.cisco",
          "match": "(?i)\\b(management|mgmt)((\\d+[\\./])*)?(\\d+)?\\b"
        }
      }
    },
    "numeric": {
      "patterns": [{ "include": "#integer" }, { "include": "#hex" }],
      "repository": {
        "hex": {
          "name": "constant.numeric.hex.cisco",
          "match": "\\b0x[0-9A-Fa-f]+\\b|(?<![:\\-.a-zA-Z])[0-9A-Fa-f]{2}(?![:\\-.0-9A-Fa-f]|[a-zA-Z])"
        },
        "integer": {
          "name": "constant.numeric.integer.cisco",
          "match": "(?<!\\.)\\b\\d+\\b(?![\\.\\:@])"
        }
      }
    },
    "comment": {
      "patterns": [
        {
          "name": "comment.line.config.cisco",
          "match": "^\\s*[!#].*"
        },
        {
          "name": "comment.line.config.cisco",
          "begin": "(?<=\\s)[!#]",
          "end": "$"
        }
      ]
    },
    "address": {
      "patterns": [
        { "include": "#mac" },
        { "include": "#cidr" },
        { "include": "#ipv4-AD" },
        { "include": "#ipv6-AD" },
        { "include": "#ipv4-full" },
        { "include": "#ipv6-full" },
        { "include": "#ipv6-condensed" }
      ],
      "repository": {
        "mac": {
          "patterns": [
            {
              "name": "keyword.other.address.mac.cisco",
              "comment": "Standard MAC format with enforced consistent separator: AA:BB:CC:DD:EE:FF / AA-BB-CC-DD-EE-FF / AA.BB.CC.DD.EE.FF",
              "match": "\\b([0-9a-fA-F]{2})([:.-])([0-9a-fA-F]{2})(\\2)([0-9a-fA-F]{2})(\\2)([0-9a-fA-F]{2})(\\2)([0-9a-fA-F]{2})(\\2)([0-9a-fA-F]{2})\\b",
              "captures": {
                "2": { "name": "punctuation.separator.cisco" },
                "4": { "name": "punctuation.separator.cisco" },
                "6": { "name": "punctuation.separator.cisco" },
                "8": { "name": "punctuation.separator.cisco" },
                "10": { "name": "punctuation.separator.cisco" }
              }
            },
            {
              "name": "keyword.other.address.mac.cisco",
              "comment": "Cisco dotted MAC format: AABB.CCDD.EEFF",
              "match": "\\b([0-9a-fA-F]{4})([.])([0-9a-fA-F]{4})([.])([0-9a-fA-F]{4})\\b",
              "captures": {
                "2": { "name": "punctuation.separator.cisco" },
                "4": { "name": "punctuation.separator.cisco" }
              }
            }
          ]
        },
        "cidr": {
          "name": "keyword.other.address.cidr.cisco",
          "match": "(?<=(?:(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)|[0-9a-fA-F]{1,4}|::)\\/)(?:12[0-8]|1[01]\\d|[1-9]?\\d)\\b"
        },
        "ipv4-AD": {
          "name": "constant.numeric.ipv4-AD.cisco",
          "match": "(?<=\\b(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\.(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\.(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\.(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\s)(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]\\d?)$"
        },
        "ipv6-AD": {
          "name": "constant.numeric.ipv6-AD.cisco",
          "match": "(?<=(?:(?:[0-9a-fA-F]{1,4}:)*[0-9a-fA-F]{0,4}::(?:[0-9a-fA-F]{1,4}:)*[0-9a-fA-F]{0,4}|(?:[0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}|::)\\s)(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]\\d?)$"
        },
        "ipv4-full": {
          "name": "keyword.other.address.ipv4.full.cisco",
          "match": "\\b(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)(\\.)(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)(\\.)(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)(\\.)(25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)\\b",
          "captures": {
            "2": { "name": "punctuation.separator.cisco" },
            "4": { "name": "punctuation.separator.cisco" },
            "6": { "name": "punctuation.separator.cisco" }
          }
        },
        "ipv6-full": {
          "name": "keyword.other.address.ipv6.full.cisco",
          "match": "\\b([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})(:)([0-9A-Fa-f]{1,4})\\b",
          "captures": {
            "2": { "name": "punctuation.separator.cisco" },
            "4": { "name": "punctuation.separator.cisco" },
            "6": { "name": "punctuation.separator.cisco" },
            "8": { "name": "punctuation.separator.cisco" },
            "10": { "name": "punctuation.separator.cisco" },
            "12": { "name": "punctuation.separator.cisco" },
            "14": { "name": "punctuation.separator.cisco" }
          }
        },
        "ipv6-condensed": {
          "comment": "IPv6 condensed format with :: compression. Matches addresses with hex groups and :: notation.",
          "match": "(?:^|\\s|(?<=[^0-9a-fA-F:]))(?:(?:[0-9a-fA-F]{1,4}:)*[0-9a-fA-F]{0,4}::(?:[0-9a-fA-F]{1,4}:)*[0-9a-fA-F]{0,4}|(?:[0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}|::)(?=\\s|[^0-9a-fA-F:]|$)",
          "captures": {
            "0": {
              "patterns": [
                {
                  "match": ":",
                  "name": "punctuation.separator.cisco"
                },
                {
                  "match": "[0-9a-fA-F]+",
                  "name": "keyword.other.address.ipv6.condensed.cisco"
                }
              ]
            }
          }
        }
      }
    },
    "command_hostname": {
      "patterns": [
        {
          "name": "meta.function-call.command_hostname.user-mode.cisco",
          "match": "^(?!\\s)[0-9a-zA-Z\\-]+(\\(.*\\))?#"
        },
        {
          "name": "meta.function-call.command_hostname.privileged-mode.cisco",
          "match": "^(?!\\s)[0-9a-zA-Z\\-]+(\\(.*\\))?>"
        }
      ]
    },
    "param1": {
      "patterns": [
        {
          "name": "punctuation.config-param.first.cisco",
          "match": "(?<=^\\s*)\\S+"
        }
      ]
    },
    "param2": {
      "patterns": [
        {
          "name": "punctuation.config-param.second.cisco",
          "match": "(?<=^\\s*\\S+\\s+)\\S+"
        }
      ]
    },
    "vrf": {
      "patterns": [
        {
          "comment": "((?<=vrf\\s)(?!.*forwarding)|(?<=vrf\\sforwarding\\s))\\S+",
          "match": "(?i)(vrf)\\s((forwarding|definition)\\s)?(\\S+)",
          "captures": {
            "1": { "name": "entity.name.class.vrf.declaration.cisco" },
            "2": { "name": "entity.other.vrf.forwarding.cisco" },
            "3": { "name": "entity.other.vrf.definition.cisco" },
            "4": { "name": "entity.name.tag.vrf.vrf-name.cisco" }
          }
        }
      ]
    },
    "string": {
      "patterns": [
        { "include": "#hostname" },
        { "include": "#description" },
        { "include": "#remark" },
        { "include": "#domain-name" },
        { "include": "#username" },
        { "include": "#name" },
        { "include": "#logging-system-message" },
        { "include": "#password" },
        { "include": "#secret" },
        { "include": "#banner" }
      ],
      "repository": {
        "description": {
          "match": "(?i)(?<=(description))\\s(.*)$",
          "name": "string.other.description.cisco"
        },
        "remark": {
          "match": "(?i)(?<=remark)\\s(.*)$",
          "name": "string.other.remark.cisco"
        },
        "hostname": {
