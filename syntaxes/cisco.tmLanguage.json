					"match": "(^|\\s)(timeout|inactive|exec-timeout|session-timeout|outside|out|exceed-action|drop|expired|failure|stop-record|boot-end-marker|error|reset|end|unassigned|unset|shutdown|system-shutdown|exit|none|limit|down|removal|removed|on-failure|cause|gbic-invalid|off|clear|standby|violation|misconfig|exit-address-family|exception|crashinfo|zeroize|erase|ignore|max-fail|theft|delete)(?=\\b|$)"
				},
				{
					"name": "keyword.other.config-keyword.group4.cisco",
					"match": "(^|\\s)(addon|activate|active|allow|allowed|authenticate|authentication-key|auto-copy|auto-upgrade|binding|boot-start-marker|both|clean|commit|conform-action|complete|detail|desirable|enable|enabled|established|full|generate|gt|in|insertion|optimize|on|on-success|permit|receive|relay|reload|RO|RW|set|show|send-community|success|trust|transmit|trusted-key|update-source|verify|YES|yes)(?=\\b|$)"
				},
				{
					"name": "keyword.other.config-keyword.group5.cisco",
					"match": "(^|\\s)(address-family|allow-hint|automatic|buffered|BVI|channel-misconfig|debugging|default-gateway|Dot11Radio|etherchannel|general-keys|if-authenticated|inspection|interval|IPDT_POLICY|label|link-local|lookup|managed-config-flag|md5|motd|mtu|negotiation|no-autoconfig|option|other-config-flag|packet|portfast|provision|renumber|route-target|scrypt|smart|snooping|standard|startupconfig|sticky|stopbits|terminal|transparent)(?=\\b|$)"
				},
				{
					"name": "keyword.other.config-keyword.group6.cisco",
					"match": "(^|\\s)(all|auto-summary|bgp|default|dscp|echo|eigrp|fragments|ftp|ftp-data|gopher|http|ident|imap|isakmp|ipv4|ipv6|lacp|log-input|nntp|ospf|pagp|ping|pop3|rip|scp|snmp|snmp-server|smtp|sonet|ssh|telnet|tcp|tftp|www)(?=\\b|$)"
				}
			]
		},
		"interface_type": {
			"patterns": [
				{
					"include": "#ethernet"
				},
				{
					"include": "#wireless"
				},
				{
					"include": "#loopback"
				},
				{
					"include": "#port-channel"
				},
				{
					"include": "#tunnel"
				},
				{
					"include": "#vlan"
				},
				{
					"include": "#null"
				},
				{
					"include": "#serial"
				},
				{
					"include": "#cellular"
				},
				{
					"include": "#virtual-template"
				},
				{
					"include": "#dialer"
				},
				{
					"include": "#async"
				},
				{
					"include": "#bri"
				},
				{
					"include": "#bvi"
				},
				{
					"include": "#app"
				},
				{
					"include": "#management"
				}
			],
			"repository": {
				"ethernet": {
					"name": "entity.name.class.interface.ethernet.cisco",
					"match": "\\b(([Ff]ast|([Ff]ive|[Tt]en|[Tt]wenty[Ff]ive|[Ff](orty|ifty)|([Tt]wo)?[Hh]undred)?[Gg](ig)?(abit)?)?[Ee](th)?(ernet)?|Fa|Gi|Te|Fo|Fi|[Gg])(((\\d+[\\./])*)?\\d+(\\.\\d+)?)(-\\d+(\\.\\d+)?)?\\b"
				},
				"wireless": {
					"name": "entity.name.class.interface.wireless.cisco",
					"match": "\\b([Dd]ot11[Rr]adio)(([0-1]([\\./]\\d)?(\\.\\d+)?)\\b(\\.\\d+)?)?\\b"
				},
				"loopback": {
					"name": "entity.name.class.interface.loopback.cisco",
					"match": "\\b([Ll]oopback|Lo)(\\d+)?\\b"
				},
				"port-channel": {
					"name": "entity.name.class.interface.portchannel.cisco",
					"match": "\\b([Pp]ort-?[Cc]hannel|Po)(12[0-8]|1[0-1][0-9]|[1-9][0-9]|[1-9])?\\b"
				},
				"tunnel": {
					"name": "entity.name.class.interface.tunnel.cisco",
					"match": "\\b([Tt]unnel|Tu)(\\d+)?\\b"
				},
				"vlan": {
					"name": "entity.name.class.interface.vlan.cisco",
					"match": "\\b(VLAN|[Vv]lan)(409[0-4]|40[0-8][0-9]|[1-3][0-9]{3}|[1-9][0-9]{1,2}|[1-9])?\\b"
				},
				"null": {
					"name": "entity.name.class.interface.null.cisco",
					"match": "\\b([Nn]ull)(\\d+)?\\b"
				},
				"serial": {
					"name": "entity.name.class.interface.serial.cisco",
					"match": "\\b([Ss]erial|[Ss])(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b"
				},
				"cellular": {
					"name": "entity.name.class.interface.cellular.cisco",
					"match": "\\b([Cc]ellular)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b"
				},
				"virtual-template": {
					"name": "entity.name.class.interface.virtual-template.cisco",
					"match": "\\b([Vv]irtual-[Tt]emplate)(\\d+)?\\b"
				},
				"dialer": {
					"name": "entity.name.class.interface.dialer.cisco",
					"match": "\\b([Dd]ialer)(\\d+)?\\b"
				},
				"async": {
					"name": "entity.name.class.interface.async.cisco",
					"match": "\\b([Aa]sync)(\\d+)?\\b"
				},
				"bri": {
					"name": "entity.name.class.interface.bri.cisco",
					"match": "\\b(BRI)(\\d+)?\\b"
				},
				"bvi": {
					"name": "entity.name.class.interface.bvi.cisco",
					"match": "\\b(BVI)(\\d+)?\\b"
				},
				"app": {
					"name": "entity.name.class.interface.app.cisco",
					"match": "\\b(([Aa]pp?[Gg](ig)?(abit)?)?[Ee](th)?(ernet)?|[Aa]p)(((\\d+[\\./])*)?\\d+(\\.\\d+)?)?\\b"
				},
				"management": {
					"name": "entity.name.class.interface.management.cisco",
					"match": "\\b(Management|[Mm]gmt)((\\d+[\\./])*)?(\\d+)?\\b"
				}
			}
		},
		"numeric": {
			"patterns": [
				{
					"include": "#integer"
				},
				{
					"include": "#hex"
				}
			],
			"repository": {
				"hex": {
					"name": "constant.numeric.hex.cisco",
					"match": "\\b(0x)?[0-9A-F]{2}+?\\b"
				},
				"integer": {
					"name": "constant.numeric.integer.cisco",
					"match": "\\b-?\\d+\\b"
				}
			}
		},
		"comment": {
			"patterns": [
				{
					"name": "comment.line.config.cisco",
					"match": "^\\s*[!#].*"
				}
			]
		},
