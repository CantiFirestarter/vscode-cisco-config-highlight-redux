						"6": { "name": "punctuation.separator.cisco" },
						"7": { "name": "keyword.other.address.ipv6.condensed.cisco" },
						"8": { "name": "punctuation.separator.cisco" },
						"9": { "name": "keyword.other.address.ipv6.condensed.cisco" },
						"10": { "name": "punctuation.separator.cisco" },
